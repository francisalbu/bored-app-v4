{
  "expo": {
    "name": "Bored Tourist",
    "slug": "bored-tourist",
    "version": "1.0.1",
    "runtimeVersion": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "app.rork.bored-explorer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F4E04D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.bored-explorer",
      "config": {
        "googleMapsApiKey": "YOUR_IOS_API_KEY"
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.rork.boredtourist.oauth",
            "CFBundleURLSchemes": [
               "app.rork.bored-explorer",
               "boredtourist",
              "com.googleusercontent.apps.2440322136358i7hsc8mco161ahd0bc5jgnnnlcsc851"
            ]
          }
        ]
      },
      "associatedDomains": [
        "applinks:hnivuisqktlrusyqywaz.supabase.co"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F4E04D"
      },
      "package": "app.rork.bored_explorer",
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_ANDROID_API_KEY"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location to show experiences near you."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.app.rork.bored-explorer",
          "enableGooglePay": true
        }
      ],
      [
        "expo-share-intent",
        {
          "iosShareExtensionName": "Save to Bored Tourist",
          "iosActivationRules": {
            "NSExtensionActivationSupportsWebURLWithMaxCount": 1,
            "NSExtensionActivationSupportsText": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://rork.com/"
      },
      "eas": {
        "projectId": "cd4bc13b-fb4a-4d0d-82ed-3faf6e991bba"
      },
      "googleAiKey": "AIzaSyAlvnCcn8ndC6avTq2BlW7LJ-H3VgCEAk4"
    },
    "owner": "boredtourist",
    "updates": {
      "url": "https://u.expo.dev/cd4bc13b-fb4a-4d0d-82ed-3faf6e991bba"
    }
  }
}
